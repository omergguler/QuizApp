@page "/profile/library"
@model PropayTest.Pages.Library.LibraryModel
@{
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>My Library</title>
    <link rel="stylesheet" href="~/css/Library.css" />

    <!-- Include Popper.js before Bootstrap -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.11.8/umd/popper.min.js"></script>
    <!-- Include Bootstrap CSS and JS -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>


</head>

<body>
    @if (Model.User != null)
    {
        @* <h1>My Library</h1>
    <h3>Library of @Model.User.FullName</h3> *@
        <div class="custom-container">
            <div class="questions item">My Questions</div>
            <div class="quizzes item">My Quizzes</div>
            <div class="achievements item">Achievements</div>
            <div class="favourites item">Favourites</div>
            <div class="bookmarks item">Bookmarks</div>
        </div>
        

    }
    else
    {
        <p>User not found or you are not authorized to view this profile.</p>
    }






    <script src="https://unpkg.com/ionicons@4.5.10-0/dist/ionicons.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.min.js"></script>







</body>


</html>
